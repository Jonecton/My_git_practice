<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:gravity="center"
    tools:context=".Healthcare">

    <Button
        android:id="@+id/btn_txins"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Texas Health Insurance" />

    <Button
        android:id="@+id/btn_txhhs"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Texas Health and Human Services" />

    <Button
        android:id="@+id/btn_fedins"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Healthcare.gov" />

</LinearLayout>
